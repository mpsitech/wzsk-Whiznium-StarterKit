include ../Makefile.inc

CPP = 
CPPFLAGS = --sysroot=${SYSROOT} -DWZSKCMBD

STATLIB = 
STATLIBFLAGS = rcs

INCPATH = -I=$(BUILDROOT)/dbswzsk -I=$(BUILDROOT)/wzskcmbd/VecWzsk -I=$(BUILDROOT)/wzskcmbd/IexWzsk -I=$(BUILDROOT)/wzskcmbd
INCPATH += -I=${WHIZSDKROOT}/build -I=${WHIZSDKROOT}/build/ezdevwskd -I=/usr/include -I=/usr/include/libxml2

OBJS = CrdWzskUsr.o PnlWzskUsrRec.o PnlWzskUsrDetail.o PnlWzskUsrAAccess.o QryWzskUsrAAccess.o PnlWzskUsr1NSession.o QryWzskUsr1NSession.o PnlWzskUsrMNUsergroup.o QryWzskUsrMNUsergroup.o PnlWzskUsrHeadbar.o PnlWzskUsrList.o QryWzskUsrList.o

LIB = libcrdwzskusr

.SUFFIXES: .cpp

.cpp.o :
	$(CPP) $(CPPFLAGS) $(INCPATH) -o $@ -c $<

all:	$(LIB)

$(LIB):	$(OBJS)
	$(STATLIB) $(STATLIBFLAGS) $(LIB).a $(OBJS)
	rm -f *.o

install:
	mv $(LIB).a ${SYSROOT}$(LIBROOT)/wzskcmbd/

